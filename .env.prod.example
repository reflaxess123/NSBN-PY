# Продакшн конфигурация
# Скопируйте в .env.prod и заполните реальными значениями

# База данных PostgreSQL
DATABASE_URL=postgresql://postgres:your_strong_password@db:5432/nareshka
POSTGRES_DB=nareshka
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_strong_password

# Redis
REDIS_URL=redis://redis:6379/0

# JWT и безопасность
SECRET_KEY=your-super-secret-key-here-change-me
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Сервер
PORT=4000
HOST=0.0.0.0
DEBUG=False

# WebDAV (если используется)
WEBDAV_URL=your_webdav_url
WEBDAV_USERNAME=your_webdav_username
WEBDAV_PASSWORD=your_webdav_password

# CORS
# Замените на ваши домены
ALLOWED_ORIGINS=["https://yourdomain.com","https://api.yourdomain.com"] 